{
  "name": "debe",
  "version": "1.36.0",
  "description": "built on totem - does everything but eat",
  "main": "debe.js",
  "scripts": {
    "startdbs": "source ./maint.sh startdbs",
    "setdebug": "source ./maint.sh config debug",
    "setoper": "source ./maint.sh config oper",
    "setprot": "source ./maint.sh config prot",
    "setprod": "source ./maint.sh config prod",
    "verminor": "npm version minor -f",
    "vermajor": "npm version major -f",
    "relink": "cd node_modules; for f in doc atomic enums totem blog skin dogs randpr pipe geohack reader jsdb man; do ln -s ../../$f $f; done",
    "redoc": "jsdoc2md --template README.hbs --files debe.js >README.md; git commit -am 'redoc'; git  push agent master",
    "genio": "node debe.js blog http://localhost:8080/totemblog.view ./stash/index.html; cd stash; git commit -am 'reblog'; git push agent master",
    "raster": "node debe.js blog ${npm_config_in} ${npm_config_out}",
    "lab": "node debe.js lab",
    "admin": "node debe.js admin"
  },
  "repository": {
    "type": "git",
    "url": "git@git.geointapps.org:acme/debe.org"
  },
  "author": "ACME Software Disposal Service",
  "license": "ISC",
  "dependencies": {
    "ajv": "^6.10.2",
    "archiver": "^3.1.1",
    "feed": "^4.1.0",
    "html-minifier": "^4.0.0",
    "i18n-abide": "0.0.26",
    "imap": "^0.8.19",
    "jade-filters": "^1.1.0",
    "jshint": "^2.13.1",
    "marked": "^0.6.3",
    "officegen": "^0.4.8",
    "openpgp": "^4.10.9",
    "optimist": "^0.6.1",
    "prettydiff": "^101.2.6",
    "request": "^2.88.0",
    "socket.io": "^2.3.0",
    "socket.io-clusterhub": "^0.2.1",
    "string-similarity": "^1.2.2",
    "tar-stream": "^2.1.0",
    "togeojson": "^0.16.0",
    "tokml": "^0.4.0",
    "uglify-js": "^3.14.3"
  }
}
